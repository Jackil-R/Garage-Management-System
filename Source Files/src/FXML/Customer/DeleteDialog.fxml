<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="140.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Customer.DeleteDialogController">
   <children>
      <VBox layoutX="100.0" layoutY="24.0" maxHeight="+Infinity" maxWidth="+Infinity" minHeight="-Infinity" prefWidth="400.0" spacing="7.0">
         <children>
            <Label fx:id="headerMessageLbl" text="Customer Removal" textAlignment="LEFT" wrapText="true">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="bodyMessageLbl" textAlignment="LEFT" wrapText="true">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <HBox layoutX="311.0" layoutY="101.0" maxHeight="-Infinity" maxWidth="+Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="175.0">
         <children>
            <Button fx:id="cancelBtn" cancelButton="true" minWidth="80.0" mnemonicParsing="false" onAction="#cancelDelete" text="Cancel" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button fx:id="deleteBtn" minWidth="80.0" mnemonicParsing="false" onAction="#deleteAccount" text="Delete" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="14.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ImageView fitHeight="79.0" fitWidth="64.0" layoutX="24.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/delete_icon.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
